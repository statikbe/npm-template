{
    "name": "@statikbe/template",
    "version": "1.0.0",
    "description": "A template for creating NPM modules",
    "license": "MIT",
    "repository": "statikbe/npm-template",
    "main": "src/index.js",
    "keywords": [
        "npm",
        "package",
        "statikbe"
    ],
    "devDependencies": {
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10"
    },
    "scripts": {
        "dev": "node_modules/webpack/bin/webpack.js --env.NODE_ENV=development --mode=development --progress --hide-modules",
        "watch": "npm run dev -- --watch",
        "prod": "node_modules/webpack/bin/webpack.js --env.NODE_ENV=production --mode=production --no-progress --hide-modules"
    }
}